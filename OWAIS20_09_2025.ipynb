        "# Abstraction Example\n",
        "from abc import ABC, abstractmethod\n",
        "class Vehicle(ABC):\n",
        "    def sound(self):\n",
        "        pass\n",
        "\n",
        "class Car (Vehicle):\n",
        "    def sound(self):\n",
        "        print(\"I'm driving a Car\")\n",
        "\n",
        "\n",
        "Obj = Car()\n",
        "Obj.sound()\n"
      ]
